
//even
public class EvenDisplay implements Runnable {
    @Override
    public void run(){
        int count=0;
        int even=2;
      while(count<20){
            System.out.println("even:"+even);
            even=even+2;
            count++;
        }
    }
}
//odd 
public class OddDisplay implements Runnable{
    @Override
    public void run(){
        int odd=1;
        int count=0;
        while (count<20){
            System.out.println("odd:"+odd);
            odd=odd+2;
            count++;
        }
    }
}
//evenodd display
public class OddEvenDisplay {
    public static void main(String[] args) {
        EvenDisplay ed=new EvenDisplay();
        OddDisplay od=new OddDisplay();
        Thread te=new Thread(ed);
        Thread to=new Thread(od);
        te.setPriority(Thread.MAX_PRIORITY);
        to.setPriority(Thread.MIN_PRIORITY);
        te.start();
        to.start();
    }
}
