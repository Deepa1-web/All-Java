

public class Addition {


        public void add(int a,int b){
            System.out.println("sum="+(a+b));
        }
        public void add(int a,int b,int c){
            System.out.println("sum is :"+(a+b+c));
        }
     public static void main(String... args){
            Addition a=new Addition();
            a.add(3,4);
            a.add(2,4,5);
     }
    }
